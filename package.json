{
  "name": "n8n-nodes-jobber",
  "version": "0.0.1",
  "description": "n8n nodes for Jobber",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://randomcpu.com",
  "author": {
    "name": "Dylan Corrales",
    "email": "deathcamel57@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DeathCamel58/n8n-nodes-jobber.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "nodelinter": "nodelinter --target=./",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/JobberOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/JobberNode/JobberRawQuery.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint-plugin-n8n-nodes-base": "^1.12.1",
    "gulp": "^4.0.2",
    "n8n-core": "^0.161.2",
    "n8n-workflow": "^0.143.2",
    "nodelinter": "^0.1.19",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4"
  }
}
